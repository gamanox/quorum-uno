<% @page_title = "UserAuth | Login" -%>
<div class= "Sign_Form">
  <h1>Log in</h1>
  <%= form_tag :url=> '/sessions/login_attempt',method: :post  do %>
    <p>Username:</br> <%= text_field_tag "uname" %></p> 
    <p>Password:</br> <%= password_field_tag "password" %></p> 
    <%= submit_tag("Log In") %>
  <% end %>
</div>